{
  "compilerOptions": {
    "target": "ESNext", // or at least ES6
    "module": "ESNext", // Ensure this is set to ESNext or ES6
    "lib": ["ESNext", "DOM"],
    "types": ["cypress", "node"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["../node_modules/cypress", "**/*.ts"]
}
